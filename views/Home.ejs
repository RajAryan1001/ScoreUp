    <script>
  // Wait for DOM to load
  document.addEventListener("DOMContentLoaded", () => {
    // Fade and slide in the left section
    gsap.from(".hero-left2", {
      opacity: 0,
      x: -100,
      duration: 1.2,
      ease: "power2.out"
    });

    // Fade and slide in the right section
    gsap.from(".hero-right2", {
      opacity: 0,
      x: 100,
      duration: 1.2,
      ease: "power2.out",
      delay: 0.5
    });

    // Animate text elements individually
    gsap.from(".hero-title2", {
      opacity: 0,
      y: 30,
      duration: 1,
      delay: 0.8
    });

    gsap.from(".hero-subtext2", {
      opacity: 0,
      y: 30,
      duration: 1,
      delay: 1
    });

    gsap.from(".demo2", {
      opacity: 0,
      y: 30,
      duration: 1,
      delay: 1.2
    });

    // Image curve animation (optional)
    gsap.from(".student-img2", {
      clipPath: "ellipse(0% 0% at 100% 50%)",
      duration: 1.5,
      ease: "power2.out",
      delay: 0.6
    });
  });
</script>

